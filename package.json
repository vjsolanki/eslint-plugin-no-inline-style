{
	"name": "eslint-plugin-jsx-no-inline-style",
	"description": "ESLint Plugin for Validating JSX Nesting",
	"version": "0.0.2",
	"author": "Vijay Solanki",
	"main": "cjs/index.js",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/vjsolanki/eslint-plugin-no-inline-style.git"
	},
	"scripts": {
		"clean": "rimraf cjs",
		"build": "tsc -b",
		"prettier": "prettier . -w",
		"test": "jest",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"files": [
		"README.md",
		"package.json",
		"dist",
		"assets"
	],
	"keywords": [
		"eslint",
		"typescript",
		"inline styles"
	],
	"devDependencies": {
		"@rollup/plugin-commonjs": "^22.0.1",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@types/estree-jsx": "^0.0.2",
		"@typescript-eslint/parser": "^5.57.0",
		"eslint": "^8.37.0",
		"jest": "26.6.3",
		"prettier": "2.2.1",
		"pretty-quick": "3.1.0",
		"rimraf": "3.0.2",
		"rollup": "^2.76.0",
		"rollup-plugin-ts": "^3.0.2",
		"ts-jest": "26.4.4",
		"typescript": "^5.0.3"
	},
	"peerDependencies": {
		"eslint": ">=4.0.0"
	},
	"jest": {
		"globals": {
			"ts-jest": {
				"diagnostics": false
			}
		},
		"transform": {
			"^.+\\.ts$": "ts-jest"
		},
		"testRegex": "(src/.*\\.test)\\.ts$",
		"testPathIgnorePatterns": [
			"/node_modules/",
			"\\.d\\.ts$",
			"lib/.*"
		],
		"moduleFileExtensions": [
			"js",
			"ts",
			"json"
		]
	},
	"dependencies": {
		"@rollup/plugin-json": "^6.0.0",
		"@typescript-eslint/utils": "^5.61.0"
	}
}
